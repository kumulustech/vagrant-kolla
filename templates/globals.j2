#kolla_internal_vip_address: "{{ansible_default_ipv4.address}}"
#network_interface: "{{ansible_default_ipv4.interface}}"
kolla_internal_vip_address: "{{ansible_enp0s8.ipv4.address}}"
network_interface: "enp0s8"
neutron_external_interface: "ext"
enable_haproxy: "no"
#enable_central_logging: "yes"
kolla_base_distro: "ubuntu"
kolla_install_type: "source"
openstack_release: "rocky"
#enable_ceph: "yes"
#enable_ceph_rgw: "yes"
#ceph_enable_cache: "yes"
#ceph_pool_type: "erasure"
#enable_ceph_rgw_keystone: "yes"
#glance_backend_ceph: "yes"
#enable_cinder: "yes"
#cinder_backup_driver: "swift"
#external_journal: false
#ceph_erasure_profile: "k=3 m=1 ruleset-failure-domain=host"
enable_openvswitch: "no"
neutron_plugin_agent: "linuxbridge"
keystone_token_provider: 'fernet'
fernet_token_expiry: 86400
